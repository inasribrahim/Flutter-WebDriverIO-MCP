{
  "name": "ios-flutter-automation-ci",
  "version": "1.0.0",
  "description": "iOS Flutter automation tests with GitHub Actions CI and Allure reporting",
  "main": "ios-flutter-test-runner.js",
  "scripts": {
    "test": "node ios-flutter-test-runner.js single",
    "test:suite": "node ios-flutter-test-runner.js suite",
    "test:ios": "node ios-flutter-test-runner.js single",
    "test:login": "node ios-flutter-test-runner.js single demouser demopass123",
    "test:custom": "node ios-flutter-test-runner.js single",
    "report:html": "echo 'Opening HTML report...' && open allure-results/test-report.html",
    "report:allure": "allure generate allure-results --clean -o allure-report && allure open allure-report",
    "clean": "rm -rf allure-results screenshots allure-report || echo 'Cleaned report directories'"
  },
  "keywords": ["flutter", "appium", "allure", "testing", "automation", "mobile"],
  "author": "Flutter Test Automation",
  "license": "MIT",
  "dependencies": {
    "appium-flutter-finder": "^0.2.0",
    "webdriverio": "^9.20.0"
  },
  "devDependencies": {
    "allure-js-commons": "^3.4.2",
    "allure-mocha": "^3.4.2",
    "fs-extra": "^11.3.2",
    "mocha": "^11.7.5",
    "path": "^0.12.7"
  }
}
